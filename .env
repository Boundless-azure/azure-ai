# 应用环境
NODE_ENV=development
PORT=3000

# 数据库配置
DB_TYPE=postgres
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=azure_ai_user
DB_PASSWORD=azure_ai_password
DB_DATABASE=azure_ai_dev

# Redis 配置
REDIS_HOST=localhost
REDIS_PORT=6379

# JWT 配置
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=7d

# AI 模型配置
OPENAI_API_KEY=your-openai-api-key
AZURE_OPENAI_ENDPOINT=your-azure-openai-endpoint
AZURE_OPENAI_API_KEY=your-azure-openai-api-key

# 日志配置
LOG_LEVEL=none

# 代理配置
AI_PROXY_ENABLED=true
# Proxy URL, e.g., http://127.0.0.1:10808
AI_PROXY_URL=http://127.0.0.1:10808
# Optional: hosts to bypass proxy, comma-separated
AI_PROXY_NO_PROXY=localhost,127.0.0.1

# 测试配置
RUN_AI_TESTS=true
